{
	"name": "ddsaform",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"obfuscate": "javascript-obfuscator build --output build-obf",
		"android": "npm run build && npm run obfuscate && npx cap sync android",
		"serenity": "failsafe serenity:clean wdio serenity:report",
		"serenity:clean": "rimraf target",
		"wdio": "wdio run ./wdio.conf.ts",
		"serenity:report": "serenity-bdd run"
	},
	"devDependencies": {
		"@capacitor/cli": "^7.4.2",
		"@serenity-js/assertions": "^3.32.4",
		"@serenity-js/console-reporter": "^3.32.4",
		"@serenity-js/core": "^3.32.4",
		"@serenity-js/mocha": "^3.32.4",
		"@serenity-js/rest": "^3.32.4",
		"@serenity-js/serenity-bdd": "^3.32.4",
		"@serenity-js/web": "^3.32.4",
		"@serenity-js/webdriverio": "^3.32.4",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@testing-library/webdriverio": "^3.2.1",
		"@types/mocha": "^10.0.10",
		"@types/node": "^24.1.0",
		"@wdio/appium-service": "^9.18.4",
		"@wdio/cli": "^9.18.4",
		"@wdio/local-runner": "^9.18.4",
		"@wdio/spec-reporter": "^9.18.0",
		"@wdio/visual-service": "^9.0.0",
		"appium-uiautomator2-driver": "^4.2.7",
		"autoprefixer": "^10.4.21",
		"mocha": "^11.7.1",
		"npm-failsafe": "^1.3.0",
		"postcss": "^8.5.6",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"rimraf": "^6.0.1",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.0.0",
		"vite": "^7.0.4",
		"wdio-wait-for": "^3.1.0"
	},
	"dependencies": {
		"@capacitor/android": "^7.4.2",
		"@capacitor/camera": "^7.0.1",
		"@capacitor/core": "^7.4.2",
		"@capacitor/geolocation": "^7.1.4",
		"appium": "^2.19.0",
		"capacitor-plugin-permissions": "^1.0.0",
		"capacitor-secure-storage-plugin": "^0.11.0",
		"crypto-js": "^4.2.0",
		"dompurify": "^3.2.6",
		"javascript-obfuscator": "^4.1.1",
		"json-logic-js": "^2.0.5",
		"mongodb": "^6.17.0"
	}
}
