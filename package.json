{
	"name": "ddsaform",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"obfuscate": "javascript-obfuscator build --output build-obf",
		"android": "npm run build && npm run obfuscate && npx cap sync android"
	},
	"devDependencies": {
		"@capacitor/cli": "^7.4.2",
		"@sveltejs/adapter-auto": "^6.0.0",
